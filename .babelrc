{
	"presets": [
		[
			"@babel/env",
			{
				"loose": true,
				"targets": {
					"browsers": ["last 2 versions", "IE >= 11"]
				}
			}
		]
	],
	"env": {
		"esm": {
			"presets": [
				[
					"@babel/env",
					{
						"loose": true,
						"modules": false,
						"targets": {
							"browsers": ["last 2 versions", "IE >= 11"]
						}
					}
				]
			]
		},
		"test": {
			"presets": [
				[
					"@babel/env",
					{
						"useBuiltIns": "usage",
						"corejs": 3
					}
				]
			]
		}
	}
}
